import java.util.Scanner;
class Pat2
{
    public static void main(String []args)
    {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        if(n>0)
        {
            for(int i=0; i<n;i++)
            {
                for(int j=0;j>i;j++)
                {
                     System.out.print("*");
                }
            System.out.println();
            }
        }
        else
        {
            System.out.print("Invalid Input");
        }
    }
}
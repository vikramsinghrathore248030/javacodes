import java.util.Iterator;
import java.util.Scanner;
class Pat2
{
    public static void main(String []args)
    {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        if(n>0)
        {
        	for (int i = n-1; i >= 1; i=i-2) 
        	{
                for (int j = 1; j <= i; j++) 
                {
                    System.out.print(i + " ");
                }
                System.out.println();
        	}
        }
        else
        {
            System.out.print("Invalid Input");
        }
    }
}
import java.util.Iterator;
import java.util.Scanner;
class Pat2
{
    public static void main(String []args)
    {
        Scanner sc = new Scanner(System.in);
        int n=sc.nextInt();
        int sum=0;
       	for (int i =1; i>0; i++) 
        {
       		n=n%10;
       		sum = n^n+sum;
        }
       	System.out.println(sum);
    }
}